<?xml version="1.0" encoding="UTF-8"?>
<jmeterTestPlan version="1.2" properties="5.0" jmeter="5.6.3">
  <hashTree>
    <TestPlan guiclass="TestPlanGui" testclass="TestPlan" testname="04 - HEAVY Body" enabled="true">
      <stringProp name="TestPlan.comments">Sc√©nario 4: Corps lourds - 50% POST 5KB, 50% PUT 5KB</stringProp>
